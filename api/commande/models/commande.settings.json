{
  "kind": "collectionType",
  "collectionName": "commandes",
  "info": {
    "name": "Commande",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "montant": {
      "type": "decimal",
      "required": true
    },
    "etat": {
      "type": "enumeration",
      "enum": [
        "Traitement",
        "Envoi",
        "Livraison"
      ],
      "default": "Traitement",
      "required": true
    },
    "produitsCommandes": {
      "type": "component",
      "repeatable": true,
      "component": "produit.produits-commandes"
    },
    "adresseLivraison": {
      "type": "component",
      "repeatable": false,
      "component": "produit.adresse-livraison"
    },
    "clientID": {
      "type": "string",
      "required": true
    },
    "clientNom": {
      "type": "string",
      "required": true
    },
    "stripeID": {
      "type": "string",
      "private": true,
      "required": true
    }
  }
}
